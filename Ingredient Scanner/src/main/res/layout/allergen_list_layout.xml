<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="edu.sfsu.cs.orange.ocr.AllergenPreferencesActivity">

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Artificial Sweetener"
        android:id="@+id/artificial_sweetener_enabled"
        android:checked="false"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageView2"
        android:layout_toEndOf="@+id/imageView2" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Carcinogens"
        android:id="@+id/carcinogenic_enabled"
        android:checked="false"
        android:layout_below="@+id/artificial_sweetener_enabled"
        android:layout_toRightOf="@+id/imageView3"
        android:layout_toEndOf="@+id/imageView3" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dairy"
        android:id="@+id/dairy_enabled"
        android:checked="false"
        android:layout_below="@+id/carcinogenic_enabled"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="31dp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Eggs"
        android:id="@+id/egg_enabled"
        android:checked="false"
        android:layout_below="@+id/dairy_enabled"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="31dp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fish"
        android:id="@+id/fish_enabled"
        android:checked="false"
        android:layout_below="@+id/egg_enabled"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="31dp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Meat"
        android:id="@+id/meat_enabled"
        android:checked="false"
        android:layout_below="@+id/fish_enabled"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="31dp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Peanuts"
        android:id="@+id/peanut_enabled"
        android:checked="false"
        android:layout_below="@+id/meat_enabled"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="31dp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sesame"
        android:id="@+id/sesame_enabled"
        android:checked="false"
        android:layout_below="@+id/peanut_enabled"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="31dp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Shellfish"
        android:id="@+id/shellfish_enabled"
        android:checked="false"
        android:layout_below="@+id/sesame_enabled"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="31dp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sweetener"
        android:id="@+id/sweetener_enabled"
        android:checked="false"
        android:layout_below="@+id/soy_enabled"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="31dp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Trans Fat"
        android:id="@+id/trans_fat_enabled"
        android:checked="false"
        android:layout_below="@+id/sweetener_enabled"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="31dp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tree Nuts"
        android:id="@+id/tree_nut_enabled"
        android:checked="false"
        android:layout_below="@+id/trans_fat_enabled"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="31dp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Wheat"
        android:id="@+id/wheat_enabled"
        android:checked="false"
        android:layout_below="@+id/tree_nut_enabled"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="31dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:src="@drawable/sweetener"
        android:layout_above="@+id/carcinogenic_enabled"
        android:layout_toLeftOf="@+id/soy_enabled"
        android:layout_toStartOf="@+id/soy_enabled" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView3"
        android:src="@drawable/carcinogens"
        android:layout_below="@+id/artificial_sweetener_enabled"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignBottom="@+id/carcinogenic_enabled"
        android:layout_toLeftOf="@+id/artificial_sweetener_enabled"
        android:layout_toStartOf="@+id/artificial_sweetener_enabled" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView4"
        android:src="@drawable/milk"
        android:layout_below="@+id/carcinogenic_enabled"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignBottom="@+id/dairy_enabled"
        android:layout_toLeftOf="@+id/carcinogenic_enabled"
        android:layout_toStartOf="@+id/carcinogenic_enabled" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView5"
        android:src="@drawable/eggs"
        android:layout_below="@+id/dairy_enabled"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignBottom="@+id/egg_enabled"
        android:layout_toLeftOf="@+id/carcinogenic_enabled"
        android:layout_toStartOf="@+id/carcinogenic_enabled" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView6"
        android:src="@drawable/fish"
        android:layout_above="@+id/meat_enabled"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/fish_enabled"
        android:layout_alignRight="@+id/imageView5"
        android:layout_alignEnd="@+id/imageView5" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView7"
        android:src="@drawable/meat"
        android:layout_above="@+id/peanut_enabled"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/meat_enabled"
        android:layout_toLeftOf="@+id/soy_enabled"
        android:layout_toStartOf="@+id/soy_enabled" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView8"
        android:src="@drawable/peanuts"
        android:layout_below="@+id/meat_enabled"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignBottom="@+id/peanut_enabled"
        android:layout_toLeftOf="@+id/soy_enabled"
        android:layout_toStartOf="@+id/soy_enabled" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView9"
        android:src="@drawable/sesame"
        android:layout_alignTop="@+id/sesame_enabled"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_above="@+id/shellfish_enabled"
        android:layout_toLeftOf="@+id/soy_enabled"
        android:layout_toStartOf="@+id/soy_enabled" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView10"
        android:src="@drawable/shellfish"
        android:layout_below="@+id/sesame_enabled"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/soy_enabled"
        android:layout_above="@+id/soy_enabled"
        android:layout_toLeftOf="@+id/soy_enabled" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView11"
        android:src="@drawable/sweetener"
        android:layout_below="@+id/soy_enabled"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignBottom="@+id/sweetener_enabled"
        android:layout_alignRight="@+id/imageView15"
        android:layout_alignEnd="@+id/imageView15" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView12"
        android:src="@drawable/trans_fat"
        android:layout_alignBottom="@+id/trans_fat_enabled"
        android:layout_alignRight="@+id/imageView13"
        android:layout_alignEnd="@+id/imageView13"
        android:layout_below="@+id/sweetener_enabled"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView13"
        android:src="@drawable/tree_nuts"
        android:layout_below="@+id/imageView12"
        android:layout_alignBottom="@+id/tree_nut_enabled"
        android:layout_alignRight="@+id/imageView14"
        android:layout_alignEnd="@+id/imageView14"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView14"
        android:src="@drawable/wheat"
        android:layout_below="@+id/imageView13"
        android:layout_alignBottom="@+id/wheat_enabled"
        android:layout_alignRight="@+id/imageView15"
        android:layout_alignEnd="@+id/imageView15"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Soy"
        android:id="@+id/soy_enabled"
        android:checked="false"
        android:layout_below="@+id/shellfish_enabled"
        android:layout_alignLeft="@+id/shellfish_enabled"/>


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView15"
        android:src="@drawable/soy"
        android:layout_below="@+id/shellfish_enabled"
        android:layout_toStartOf="@+id/soy_enabled"
        android:layout_alignBottom="@+id/soy_enabled"
        android:layout_toLeftOf="@+id/soy_enabled" />


</RelativeLayout>